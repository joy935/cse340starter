<h1><%= title %></h1>
<div>
    <%- messages() %>
</div>
<% if (errors) { %>
    <ul class="notice">
   <% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
  <%  }) %>
   </ul>
<% } %>
<% if (wishlist === 0) { %>  <!-- Check if wishlist is empty -->
    <p>Nothing in the wishlist, really?</p>
    <a href="/inv/type/1">Browse the Custom inventory</a>
<% } else { %>
    <%- wishlist %>
<% } %>
